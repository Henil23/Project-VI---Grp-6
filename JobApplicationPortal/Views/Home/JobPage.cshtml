@model List<Job>

@{
    ViewData["Title"] = "Job openings";
}

<h1>Job Listings</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Company</th>
            <th>Posted Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var job in Model)
        {
            <tr>
                <td>@job.Title</td>
                <td>@job.Company</td>
                <td>@job.PostedDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
